{
  "name": "grid-ui",
  "version": "0.2.0",
  "description": "ndarray grid user interface",
  "main": "index.js",
  "scripts": {
    "example": "beefy -p 5000 example/app.js",
    "test": "node test/index.js | tap-spec",
    "travis-test": "npm run phantom && npm run cover && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0)",
    "phantom": "run-browser test/index.js -b | tap-spec",
    "browser": "run-browser test/index.js",
    "cover": "istanbul cover --report html --print detail ./test/index.js",
    "view-cover": "istanbul report html && google-chrome ./coverage/index.html"
  },
  "browserify": {
    "transform": [
      "cssify",
      "envify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ahdinosaur/grid-ui.git"
  },
  "author": "ahdinosaur",
  "licenses": [
    {
      "type": "AGPL-3.0",
      "url": "https://github.com/ahdinosaur/grid-ui/raw/master/LICENSE-AGPL-3.0.txt"
    },
    {
      "type": "PPL",
      "url": "https://github.com/ahdinosaur/grid-ui/raw/master/LICENSE-PPL.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/ahdinosaur/grid-ui/issues"
  },
  "homepage": "https://github.com/ahdinosaur/grid-ui",
  "testling": {
    "files": "test/index.js",
    "browsers": [
      "ie/9..latest",
      "firefox/26..latest",
      "firefox/nightly",
      "chrome/31..latest",
      "chrome/canary",
      "opera/18..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  },
  "dependencies": {
    "debug": "^1.0.4",
    "global": "^4.2.1",
    "mercury": "^6.0.1",
    "node-stringify": "0.0.18",
    "observ-ndarray": "^0.2.0"
  },
  "devDependencies": {
    "beefy": "^2.1.0",
    "coveralls": "^2.11.1",
    "cssify": "^0.5.1",
    "envify": "^2.0.1",
    "istanbul": "^0.3.0",
    "ndarray": "^1.0.15",
    "raf": "^2.0.2",
    "run-browser": "^1.3.1",
    "synthetic-dom-events": "git://github.com/Raynos/synthetic-dom-events",
    "tap-spec": "^0.2.0",
    "tape": "^2.14.0",
    "tween.js": "^0.14.0"
  }
}
